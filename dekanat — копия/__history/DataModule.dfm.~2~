  object QAdress: TFDQuery
    CachedUpdates = True
    Connection = FDConnection1
    SQL.Strings = (
      
        'select district.name_district, type_local.name_type, locality.na' +
        'me_locality, type_street.type,'
      
        '  district.n_district, district.n_region, type_local.n_type, loc' +
        'ality.id as local_id, type_street.id as type_street'
      ' from district,'
      
