  object Prikaz_KursFDSQL: TFDUpdateSQL
    Connection = FDConnection1
    InsertSQL.Strings = (
      'insert into PRIKAZ_KURS'
      '  (NOMER, N_KURS, D_PRIKAZ_KURS, N_PRIKAZ_KURS, N_FAC, VID_EDU)'
      'values'
      
        '  (:NOMER, :N_KURS, :D_PRIKAZ_KURS, :N_PRIKAZ_KURS, :N_FAC, :VID' +
        '_EDU)'
      '')
    ModifySQL.Strings = (
      'update PRIKAZ_KURS'
      'set'
      '  NOMER = :NOMER,'
      '  N_KURS = :N_KURS,'
      '  D_PRIKAZ_KURS = :D_PRIKAZ_KURS,'
      '  N_PRIKAZ_KURS = :N_PRIKAZ_KURS,'
      '  N_FAC = :N_FAC,'
      '  VID_EDU = :VID_EDU'
      'where'
      '  NOMER = :OLD_NOMER and'
      '  N_KURS = :OLD_N_KURS and'
      '  D_PRIKAZ_KURS = :OLD_D_PRIKAZ_KURS and'
      '  N_PRIKAZ_KURS = :OLD_N_PRIKAZ_KURS and'
      '  N_FAC = :OLD_N_FAC and'
      '  VID_EDU = :OLD_VID_EDU'
      '')
    DeleteSQL.Strings = (
      'delete from PRIKAZ_KURS'
      'where'
      '  NOMER = :OLD_NOMER and'
      '  N_KURS = :OLD_N_KURS and'
      '  D_PRIKAZ_KURS = :OLD_D_PRIKAZ_KURS and'
      '  N_PRIKAZ_KURS = :OLD_N_PRIKAZ_KURS and'
      '  N_FAC = :OLD_N_FAC and'
      '  VID_EDU = :OLD_VID_EDU'
      '')
    Left = 648
    Top = 56
  end
  object Fac_AllFDQuery: TFDQuery
    Connection = FDConnection1
    SQL.Strings = (
      'SELECT  Cause_pass.CAUSE, People.KURS, SUM( Pass.HOUR_PASS )'
      'FROM CAUSE_PASS Cause_pass'
      '   INNER JOIN PASS Pass'
      '   ON  (Cause_pass.N_CAUSE = Pass.N_CAUSE)  '
      '   INNER JOIN PEOPLE People'
      '   ON  (Pass.NOMER = People.NOMER)  '
      
        'WHERE (Pass.D_PASS>=:START)  AND (Pass.D_PASS<:END) AND (People.' +
        'VID_EDU=0) AND (People.N_FAC=:N_FAC) AND (People.STATUS_PEOPLE=1' +
        ')'
