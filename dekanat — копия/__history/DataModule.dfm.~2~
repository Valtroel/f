  object Group_AllFDQuery: TFDQuery
    Connection = FDConnection1
    SQL.Strings = (
      
        'SELECT People.FAM, People.NAME, People.OTCH, Cause_pass.CAUSE, S' +
        'UM( Pass.HOUR_PASS ), people.n_group, People.INC_GROUP, Cause_pa' +
        'ss.N_CAUSE'
      'FROM CAUSE_PASS Cause_pass'
      '   INNER JOIN PASS Pass'
      '   ON  (Cause_pass.N_CAUSE = Pass.N_CAUSE)  '
      '   INNER JOIN PEOPLE People'
      '   ON  (Pass.NOMER = People.NOMER)  '
      
        'WHERE (Pass.D_PASS>=:START)  AND (Pass.D_PASS<:END) AND (People.' +
        'VID_EDU=0) AND (People.N_FAC=:N_FAC) AND (People.STATUS_PEOPLE=1' +
        ')'
      
        'GROUP BY people.n_group, People.INC_GROUP, People.FAM, Cause_pas' +
        's.CAUSE, People.NAME, People.OTCH, Cause_pass.N_CAUSE'
      
        'ORDER BY people.n_group, People.INC_GROUP, People.FAM, Cause_pas' +
        's.N_CAUSE'
      '')
    Left = 976
    Top = 56
    ParamData = <
      item
        Name = 'START'
        DataType = ftTimeStamp
        ParamType = ptInput
      end
      item
        Name = 'END'
        DataType = ftTimeStamp
        ParamType = ptInput
      end
      item
        Name = 'N_FAC'
        DataType = ftSmallint
        ParamType = ptInput
      end>
