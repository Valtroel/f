    OnCalcFields = LikvidFDQueryCalcFields
    CachedUpdates = True
    Connection = FDConnection1
    UpdateObject = LikvidFDUpdateSQL
    SQL.Strings = (
      
        'SELECT People.FAM || '#39' '#39' || People.NAME || '#39' '#39' || People.OTCH as' +
        ' FIO, Prikaz_likvid.N_PRIKAZ_KIKVID, Prikaz_likvid.NOMER, Prikaz' +
        '_likvid.YEAR_UCH, Prikaz_likvid.PRIKAZ, Prikaz_likvid.STATUS_NOM' +
        'ER, Prikaz_likvid.DATE_C, Prikaz_likvid.DATE_PO, Prikaz_likvid.D' +
