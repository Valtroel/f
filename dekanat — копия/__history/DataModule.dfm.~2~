  object Priv_AddFDUpdateSQL: TFDUpdateSQL
    Connection = FDConnection1
    InsertSQL.Strings = (
      'insert into PRIV'
      
        '  (NOMER, VID_PRIV, UCH_GOD, TEXT_PRIV, DOC_PRIV, DATA_C, DATA_P' +
        'O, '
      'DESCRIBE)'
      'values'
      '  (:NOMER, :VID_PRIV, :UCH_GOD, :TEXT_PRIV, :DOC_PRIV, :DATA_C, '
      ':DATA_PO, '
      '   :DESCRIBE)'
      '')
    ModifySQL.Strings = (
      'update PRIV'
      'set'
      '  NOMER = :NOMER,'
      '  VID_PRIV = :VID_PRIV,'
      '  UCH_GOD = :UCH_GOD,'
      '  TEXT_PRIV = :TEXT_PRIV,'
      '  DOC_PRIV = :DOC_PRIV,'
      '  DATA_C = :DATA_C,'
      '  DATA_PO = :DATA_PO,'
      '  DESCRIBE = :DESCRIBE'
      'where'
      '  N_PRIV = :OLD_N_PRIV'
      '')
    DeleteSQL.Strings = (
      'delete from PRIV'
      'where'
      '  N_PRIV = :OLD_N_PRIV'
      '')
    Left = 168
    Top = 584
  end
