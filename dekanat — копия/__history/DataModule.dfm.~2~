  object DiplomREGExcelFDQuery: TFDQuery
    CachedUpdates = True
    Connection = FDConnection1
    SQL.Strings = (
      
        'SELECT d.nomer, p.data_r, p.d_prikaz_kurs,p.fam, p.name, p.otch,' +
        ' p.n_spec, p.n_specializ, p.d_prikaz_include, p.n_fac,  p.VID_ED' +
        'U, p.N_ZACHET,'
      
        'd.d_vipusk, d.qualification, d.n_protokol, d.d_protokol, d.n_dip' +
        'lom, d.seria, d.nom_diplom, d.registration, d.TYPE_DIPLOM,'
      
        'r.n_public_prikaz, r.d_public_prikaz, per.series, per.number, pe' +
        'r.personal_number, per.who_give, per.date_give'
      'From diploma d'
      ' inner join people p on (p.nomer=d.nomer)'
      ' inner join public_prikaz r on (p.nomer=r.nomer)'
      ' inner join personal_docum per on (p.nomer=per.nomer)'
      'WHERE'
      'p.N_FAC=:N_FAC '
      'AND p.VID_EDU=:EDU '
      'AND p.KURS=:KURS '
      
        'AND (p.STATUS_PEOPLE=1 or (p.STATUS_PEOPLE=6 and d.d_vipusk like' +
        ' :GOD))'
      'and not d.nom_diplom is null'
      'and r.n_type = 9'
      'order by p.fam, p.name, p.otch')
