  object RatingFDUpdateSQL: TFDUpdateSQL
    Connection = FDConnection1
    InsertSQL.Strings = (
      'insert into RATING'
      '  (N_VED_RATING, NOMER, BALL_R, R_FAC, R_KURS, R_GROUP, R_DOP)'
      'values'
      
        '  (:N_VED_RATING, :NOMER, :BALL_R, :R_FAC, :R_KURS, :R_GROUP, :R' +
        '_DOP)'
      '')
    ModifySQL.Strings = (
      'update RATING'
      'set'
      '  N_VED_RATING = :N_VED_RATING,'
      '  NOMER = :NOMER,'
      '  BALL_R = :BALL_R,'
      '  R_FAC = :R_FAC,'
      '  R_KURS = :R_KURS,'
      '  R_GROUP = :R_GROUP,'
      '  R_DOP = :R_DOP'
      'where'
      '  N_RATING = :OLD_N_RATING'
      '')
    DeleteSQL.Strings = (
      'delete from RATING'
      'where'
      '  N_RATING = :OLD_N_RATING'
      '')
    Left = 256
    Top = 440
  end
