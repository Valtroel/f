    Connection = FDConnection1
    InsertSQL.Strings = (
      'insert into SPORT'
      '  (NOMER, SPORT_CATEGORY, SUD_CATEGORY, WHO_GIVE, '
      'D_PRIKAZ_SPORT, N_PRIKAZ_SPORT, '
      '   N_VID_SPORT, WHO_GIVE_SUD, D_PRIKAZ_SUD, N_PRIKAZ_SUD)'
      'values'
      '  (:NOMER, :SPORT_CATEGORY, :SUD_CATEGORY, :WHO_GIVE, '
      ':D_PRIKAZ_SPORT, '
      '   :N_PRIKAZ_SPORT, :N_VID_SPORT, :WHO_GIVE_SUD, :D_PRIKAZ_SUD, '
      ':N_PRIKAZ_SUD)'
      '')
    ModifySQL.Strings = (
      'update SPORT'
      'set'
      '  NOMER = :NOMER,'
      '  SPORT_CATEGORY = :SPORT_CATEGORY,'
      '  SUD_CATEGORY = :SUD_CATEGORY,'
      '  WHO_GIVE = :WHO_GIVE,'
      '  D_PRIKAZ_SPORT = :D_PRIKAZ_SPORT,'
      '  N_PRIKAZ_SPORT = :N_PRIKAZ_SPORT,'
      '  N_VID_SPORT = :N_VID_SPORT,'
      '  WHO_GIVE_SUD = :WHO_GIVE_SUD,'
      '  D_PRIKAZ_SUD = :D_PRIKAZ_SUD,'
      '  N_PRIKAZ_SUD = :N_PRIKAZ_SUD'
      'where'
      '  NOMER = :OLD_NOMER and'
      '  SPORT_CATEGORY = :OLD_SPORT_CATEGORY and'
      '  N_VID_SPORT = :OLD_N_VID_SPORT'
      '')
    DeleteSQL.Strings = (
      'delete from SPORT'
      'where'
      '  NOMER = :OLD_NOMER and'
