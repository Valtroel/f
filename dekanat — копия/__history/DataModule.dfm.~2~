  object Edu_PlanFDQ: TFDQuery
    CachedUpdates = True
    Connection = FDConnection1
    SQL.Strings = (
      
        'SELECT e.N_OBJECT, o.name_object, o.name_object || '#39' '#39' || e.CLOC' +
        'K_PLAN_all || '#39#1095#39' as pp,'
      
        'e.SEMESTR, e.CLOCK_PLAN, e.YEAR_PLAN, e.N_FAC, e.ZACH_EXAM, e.VI' +
        'D_EDU_PLAN, e.SPECKURS, e.STATUS_CONTROL, e.N_PLAN'
      'FROM EDU_PLAN e'
      'left join object_plan  o on (e.n_object=o.n_object)'
      'WHERE'
      
        '(N_FAC=:N_FAC) AND (YEAR_PLAN=:PLAN) AND (SEMESTR=:SEM) AND (VID' +
        '_EDU_PLAN=:VID)'
      ''
      'ORDER BY SEMESTR, ZACH_EXAM'
      '')
    Left = 168
    Top = 440
    ParamData = <
      item
        Name = 'N_FAC'
        DataType = ftSmallint
        ParamType = ptInput
      end
      item
        Name = 'PLAN'
        DataType = ftSmallint
        ParamType = ptInput
        Value = Null
      end
      item
        Name = 'SEM'
