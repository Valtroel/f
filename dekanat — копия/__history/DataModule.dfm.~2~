  object Rating_BallFDQuery: TFDQuery
    BeforePost = Rating_BallFDQueryBeforePost
    CachedUpdates = True
    Connection = FDConnection1
    UpdateObject = Rating_BallFDUpdateSQL
    SQL.Strings = (
      
        'SELECT Rating.N_RATING, Rating.N_VED_RATING, Rating.NOMER, Ratin' +
        'g.BALL_R, Rating.R_FAC, Rating.R_KURS, Rating.R_GROUP, Rating.R_' +
        'DOP, People.FAM, People.NAME, People.OTCH, Vedomost_rating.N_VED' +
        'OMOST_R, Vedomost_rating.N_FAC, Vedomost_rating.VID_EDU, Vedomos' +
        't_rating.UCH_GOD, Vedomost_rating.SEMESTR, Vedomost_rating.SESSI' +
        'ON, Vedomost_rating.INC_GROUP, Vedomost_rating.N_SPECIALIZ, Spec' +
        'ializ.SPECIAL, Increment_group.N_GROUP, People.FAM|| '#39' '#39' || Peop' +
        'le.NAME || '#39' '#39' || People.OTCH as FIO'
      'FROM RATING Rating'
      '   INNER JOIN PEOPLE People'
      '   ON  (Rating.NOMER = People.NOMER)  '
      '   INNER JOIN VEDOMOST_RATING Vedomost_rating'
      '   ON  (Rating.N_VED_RATING = Vedomost_rating.N_VED_RATING)  '
      '   INNER JOIN SPECIALIZ Specializ'
      '   ON  (Vedomost_rating.N_SPECIALIZ = Specializ.N_SPECIALIZ)  '
      '   INNER JOIN INCREMENT_GROUP Increment_group'
      '   ON  (Vedomost_rating.INC_GROUP = Increment_group.INC_GROUP)  '
      'where (Vedomost_rating.N_FAC=:FAC)'
      '')
    Left = 696
    Top = 440
    ParamData = <
      item
        Name = 'FAC'
        DataType = ftInteger
        ParamType = ptInput
      end>
