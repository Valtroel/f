  object Result_Ball_ThreeFDQuery: TFDQuery
    CachedUpdates = True
    Connection = FDConnection1
    UpdateObject = Result_Ball_ThreeFDUpdateSQL
    SQL.Strings = (
      
        'SELECT RESULT_BALL.NOMER, OBJECT_PLAN.NAME_OBJECT, RESULT_BALL.N' +
        '_PLAN, RESULT_BALL.RESULT, RESULT_BALL.DATE_RESULT, RESULT_BALL.' +
        'STATUS_RESULT, EDU_PLAN.SEMESTR'
      'FROM EDU_PLAN'
      
        '   INNER JOIN RESULT_BALL ON (EDU_PLAN.N_PLAN = RESULT_BALL.N_PL' +
        'AN)'
      
        '   INNER JOIN OBJECT_PLAN ON (EDU_PLAN.N_OBJECT = OBJECT_PLAN.N_' +
        'OBJECT)'
      'WHERE '
      '   ('
      
        '      ((RESULT_BALL.RESULT = 3) or (RESULT_BALL.RESULT = 14) or ' +
        '(RESULT_BALL.RESULT = 15))'
      '   and '
      '      ('
      '         (EDU_PLAN.SEMESTR = :S1)'
      '      or '
      '         (EDU_PLAN.SEMESTR = :S2)'
      '      )'
      '   and '
      '      (EDU_PLAN.N_FAC = :FAC)'
      '   and '
      '      (EDU_PLAN.VID_EDU_PLAN = :EDU)'
      '   and '
