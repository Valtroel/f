  object Result_BallFDUpdateSQL: TFDUpdateSQL
    Connection = FDConnection1
    InsertSQL.Strings = (
      'insert into RESULT_BALL'
      '  (NOMER, RESULT, DATE_RESULT, STATUS_RESULT, N_PLAN)'
      'values'
      '  (:NOMER, :RESULT, :DATE_RESULT, :STATUS_RESULT, :N_PLAN)'
      '')
    ModifySQL.Strings = (
      'update RESULT_BALL'
      'set'
      '  NOMER = :NOMER,'
      '  RESULT = :RESULT,'
      '  DATE_RESULT = :DATE_RESULT,'
      '  STATUS_RESULT = :STATUS_RESULT,'
      '  N_PLAN = :N_PLAN'
      'where'
      '  NOMER = :OLD_NOMER and'
      '  RESULT = :OLD_RESULT and'
      '  DATE_RESULT = :OLD_DATE_RESULT and'
      '  N_PLAN = :OLD_N_PLAN')
    DeleteSQL.Strings = (
      'delete from RESULT_BALL'
      'where'
      '  NOMER = :OLD_NOMER and'
      '  RESULT = :OLD_RESULT and'
      '  DATE_RESULT = :OLD_DATE_RESULT and'
      '  N_PLAN = :OLD_N_PLAN')
    Left = 288
    Top = 56
  end
