      '  N_ZACHET = :N_ZACHET,'
      '  STATUS_PEOPLE = :STATUS_PEOPLE,'
      '  INC_GROUP = :INC_GROUP,'
      '  DATA_R = :DATA_R,'
      '  VID_EDU_INCLUDE = :VID_EDU_INCLUDE,'
      '  N_FOUND_INCLUDE = :N_FOUND_INCLUDE,'
      '  SCHOLARSHIP = :SCHOLARSHIP,'
