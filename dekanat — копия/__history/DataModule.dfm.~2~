        'L, :N_HOL, '
      '   :TYPE_HOL)'
      '')
    ModifySQL.Strings = (
      'update HOLIDAY'
      'set'
      '  NOMER = :NOMER,'
      '  N_PRIKAZ_HOL = :N_PRIKAZ_HOL,'
      '  D_PRIKAZ_HOL = :D_PRIKAZ_HOL,'
      '  D_BEGIN_HOL = :D_BEGIN_HOL,'
      '  D_END_HOL = :D_END_HOL,'
      '  N_HOL = :N_HOL,'
      '  TYPE_HOL = :TYPE_HOL'
      'where'
      '  NOMER = :OLD_NOMER and'
      '  N_PRIKAZ_HOL = :OLD_N_PRIKAZ_HOL and'
      '  D_PRIKAZ_HOL = :OLD_D_PRIKAZ_HOL'
      '')
    DeleteSQL.Strings = (
      'delete from HOLIDAY'
      'where'
      '  NOMER = :OLD_NOMER and'
      '  N_PRIKAZ_HOL = :OLD_N_PRIKAZ_HOL and'
      '  D_PRIKAZ_HOL = :OLD_D_PRIKAZ_HOL'
      '')
    Left = 792
