  object EncourageFDUpdateSQL: TFDUpdateSQL
    Connection = FDConnection1
    InsertSQL.Strings = (
      'insert into ENCOURAGE'
      '  (NOMER, N_TYPE_EN, WHAT, WHO_TAKE, N_PRIKAZ, D_PRIKAZ)'
      'values'
      '  (:NOMER, :N_TYPE_EN, :WHAT, :WHO_TAKE, :N_PRIKAZ, :D_PRIKAZ)'
      '')
    ModifySQL.Strings = (
      'update ENCOURAGE'
      'set'
      '  NOMER = :NOMER,'
      '  N_TYPE_EN = :N_TYPE_EN,'
      '  WHAT = :WHAT,'
      '  WHO_TAKE = :WHO_TAKE,'
      '  N_PRIKAZ = :N_PRIKAZ,'
      '  D_PRIKAZ = :D_PRIKAZ'
      'where'
      '  NOMER = :OLD_NOMER and'
      '  N_TYPE_EN = :OLD_N_TYPE_EN and'
      '  N_PRIKAZ = :OLD_N_PRIKAZ and'
      '  D_PRIKAZ = :OLD_D_PRIKAZ'
      '')
    DeleteSQL.Strings = (
      'delete from ENCOURAGE'
      'where'
      '  NOMER = :OLD_NOMER and'
      '  N_TYPE_EN = :OLD_N_TYPE_EN and'
      '  N_PRIKAZ = :OLD_N_PRIKAZ and'
      '  D_PRIKAZ = :OLD_D_PRIKAZ'
      '')
    Left = 376
    Top = 200
  end
