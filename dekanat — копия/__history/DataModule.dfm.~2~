  object Spisoc_All_StudFDQuery: TFDQuery
    Connection = FDConnection1
    SQL.Strings = (
      
        'SELECT People.NOMER, People.FAM, People.NAME, People.OTCH, Peopl' +
        'e.N_GROUP as N_GR, Increment_group.N_GROUP, Increment_group.VID_' +
        'EDU, Specializ.SPECIAL, People.POL, People.DATA_R, Vid_sport.VID' +
