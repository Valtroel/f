        'ating.CURATOR, Vedomost_rating.D_VED, Vedomost_rating.END_D_VED,' +
        ' Rating.NOMER, People.FAM, People.NAME, People.OTCH,  People.FAM' +
        ' || '#39' '#39' || People.NAME ||'#39' '#39' || People.OTCH as FIO, Increment_gr' +
        'oup.N_GROUP, Specializ.SPECIAL, Faculty.FAC1'
      'FROM VEDOMOST_RATING Vedomost_rating'
      '   INNER JOIN RATING Rating'
      '   ON  (Vedomost_rating.N_VED_RATING = Rating.N_VED_RATING)  '
      '   INNER JOIN INCREMENT_GROUP Increment_group'
      '   ON  (Vedomost_rating.INC_GROUP = Increment_group.INC_GROUP)  '
      '   INNER JOIN SPECIALIZ Specializ'
      '   ON  (Vedomost_rating.N_SPECIALIZ = Specializ.N_SPECIALIZ)  '
      '   INNER JOIN FACULTY Faculty'
      '   ON  (Vedomost_rating.N_FAC = Faculty.N_FAC)  '
      '   INNER JOIN PEOPLE People'
      '   ON  (Rating.NOMER = People.NOMER)  '
      
        'Where (Vedomost_rating.N_FAC=:FAC) AND (Vedomost_rating.N_VED_RA' +
        'TING=:N_VED)'
      'order by  People.FAM, People.NAME, People.OTCH'
      '')
    Left = 640
    Top = 344
    ParamData = <
      item
        Name = 'FAC'
        DataType = ftInteger
        ParamType = ptInput
      end
      item
        Name = 'N_VED'
        DataType = ftInteger
        ParamType = ptInput
