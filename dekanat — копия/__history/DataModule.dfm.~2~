        ', Prikaz_likvid.N_FOUND_PO'
      'FROM PRIKAZ_LIKVID Prikaz_likvid'
      '   INNER JOIN PEOPLE People'
      '   ON  (Prikaz_likvid.NOMER = People.NOMER)  '
      '   INNER JOIN TYPE_LIKVID Type_likvid'
      
