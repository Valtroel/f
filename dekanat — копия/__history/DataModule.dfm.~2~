  object Kurs_AllFDQuery: TFDQuery
    Connection = FDConnection1
    SQL.Strings = (
      
        'SELECT People.KURS, People.INC_GROUP, People.n_group, Cause_pass' +
        '.CAUSE, SUM( Pass.HOUR_PASS ), Cause_pass.N_CAUSE'
      'FROM CAUSE_PASS Cause_pass'
      '   INNER JOIN PASS Pass'
      '   ON  (Pass.N_CAUSE = Cause_pass.N_CAUSE)  '
      '   INNER JOIN PEOPLE People'
      '   ON  (People.NOMER = Pass.NOMER)  '
      
        'WHERE (Pass.D_PASS>=:START)  AND (Pass.D_PASS<:END) AND (People.' +
        'VID_EDU=0) AND (People.N_FAC=:N_FAC) AND (People.STATUS_PEOPLE=1' +
        ')'
      
        'GROUP BY People.KURS, People.n_group, People.INC_GROUP, Cause_pa' +
        'ss.CAUSE, Cause_pass.N_CAUSE'
      
        'ORDER BY People.KURS, People.n_group, People.INC_GROUP, Cause_pa' +
        'ss.N_CAUSE'
      '')
    Left = 408
    Top = 104
