  object Semestr_KursFDQuery: TFDQuery
    Connection = FDConnection1
    SQL.Strings = (
      'SELECT People.NOMER, Edu_card.D_EXAM, Edu_card.BALL'
      'FROM PEOPLE People'
      '   INNER JOIN INCREMENT_GROUP Increment_group'
      '   ON  (People.INC_GROUP = Increment_group.INC_GROUP)  '
      '   INNER JOIN EDU_CARD Edu_card'
      '   ON  (People.NOMER = Edu_card.NOMER)  '
      '   INNER JOIN VEDOMOST Vedomost'
      '   ON  (Edu_card.N_VED = Vedomost.N_VED)  '
      '   INNER JOIN EDU_PLAN Edu_plan'
      '   ON  (Vedomost.N_PLAN = Edu_plan.N_PLAN)  '
      '   INNER JOIN OBJECT_PLAN Object_plan'
      '   ON  (Edu_plan.N_OBJECT = Object_plan.N_OBJECT)  '
      'WHERE   (Increment_group.INC_GROUP = :INC_GR)  '
      '   AND  (Object_plan.NAME_OBJECT = :NAME_OBJ)  '
      '   AND  (People.STATUS_PEOPLE = 1)  '
      'AND (Edu_plan.YEAR_PLAN=:PLAN)'
      'AND (Edu_plan.SEMESTR=:SEM)'
      'AND (Edu_card.BALL IN (2,3,4,5))'
      'AND (Vedomost.invalid = 0)'
      'ORDER BY People.NOMER, Edu_card.D_EXAM'
      '')
    Left = 552
    Top = 296
    ParamData = <
      item
        Name = 'INC_GR'
        DataType = ftSmallint
        ParamType = ptInput
      end
      item
        Name = 'NAME_OBJ'
        DataType = ftString
        ParamType = ptInput
        Size = 250
      end
      item
        Name = 'PLAN'
        DataType = ftSmallint
        ParamType = ptInput
      end
      item
        Name = 'SEM'
        DataType = ftSmallint
        ParamType = ptInput
      end>
