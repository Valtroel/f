  object CK_FDQuery: TFDQuery
    Connection = FDConnection1
    SQL.Strings = (
      
        'SELECT PEOPLE.FAM ||'#39' '#39'|| PEOPLE.NAME ||'#39' '#39' || people.otch as FI' +
        'O, SPECIALIZ.SPECIAL, people.n_group,  REGIONAL_CENTRE.CENTRE, C' +
        'K.CUSTOMER,  PEOPLE.N_FAC'
      'FROM      PEOPLE'
