  object VedJournal: TFDQuery
    Connection = FDConnection1
    SQL.Strings = (
      
        'SELECT Vedomost.N_VED, Vedomost.N_VEDOMOST, Object_plan.NAME_OBJ' +
        'ECT, Increment_group.N_GROUP, Vedomost.FIO_EXAM, Vedomost.D_VED'
      'FROM VEDOMOST Vedomost'
      '   INNER JOIN EDU_PLAN Edu_plan'
      '   ON  (Vedomost.N_PLAN = Edu_plan.N_PLAN)  '
      '   INNER JOIN OBJECT_PLAN Object_plan'
      '   ON  (Edu_plan.N_OBJECT = Object_plan.N_OBJECT) '
      '   INNER JOIN INCREMENT_GROUP Increment_group'
      '   ON (Increment_group.INC_GROUP = Vedomost.INC_GROUP)  '
      
        'WHERE (Vedomost.N_FAC=:N_FAC) AND (Vedomost.INVALID = 0) AND (Ed' +
        'u_plan.VID_EDU_PLAN=:VID_EDU) '
      
        '  AND ((Vedomost.SEMESTR_VED / 2 = :KURS) OR ((Vedomost.SEMESTR_' +
        'VED - 1) / 2 = :KURS)) AND (Vedomost.STATUS=:STATUS) AND (Vedomo' +
        'st.D_VED BETWEEN :D1 AND :D2)')
    Left = 360
    Top = 912
    ParamData = <
      item
        Name = 'N_FAC'
        DataType = ftSmallint
        ParamType = ptInput
        Value = Null
      end
      item
        Name = 'VID_EDU'
        DataType = ftSmallint
        ParamType = ptInput
      end
      item
        Name = 'KURS'
        DataType = ftSmallint
        ParamType = ptInput
      end
      item
        Name = 'STATUS'
        ParamType = ptInput
      end
      item
        Name = 'D1'
        ParamType = ptInput
      end
      item
        Name = 'D2'
        ParamType = ptInput
      end>
    object VedJournalN_VED: TIntegerField
      FieldName = 'N_VED'
      Origin = 'N_VED'
      ProviderFlags = [pfInUpdate, pfInWhere, pfInKey]
    end
    object VedJournalN_VEDOMOST: TSmallintField
      FieldName = 'N_VEDOMOST'
      Origin = 'N_VEDOMOST'
    end
    object VedJournalNAME_OBJECT: TStringField
      AutoGenerateValue = arDefault
      FieldName = 'NAME_OBJECT'
      Origin = 'NAME_OBJECT'
      ProviderFlags = []
      ReadOnly = True
      Size = 250
    end
    object VedJournalN_GROUP: TStringField
      AutoGenerateValue = arDefault
      FieldName = 'N_GROUP'
      Origin = 'N_GROUP'
      ProviderFlags = []
      ReadOnly = True
      Size = 10
    end
    object VedJournalFIO_EXAM: TStringField
      FieldName = 'FIO_EXAM'
      Origin = 'FIO_EXAM'
      Size = 40
    end
    object VedJournalD_VED: TSQLTimeStampField
      FieldName = 'D_VED'
      Origin = 'D_VED'
    end
  end
  object VedListJournal: TFDQuery
    Connection = FDConnection1
    SQL.Strings = (
      
        'SELECT Vedomost.N_VED, Vedomost.N_VEDOMOST, Object_plan.NAME_OBJ' +
        'ECT, Vedomost.SEMESTR_VED||'#39', '#39'||Increment_group.N_GROUP as N_GR' +
        'OUP, Vedomost.FIO_EXAM, People.FAM||'#39' '#39'||LEFT(People.NAME,1)||'#39'.' +
        ' '#39'||LEFT(People.OTCH,1)||'#39'.'#39' as FIO, Vedomost.D_VED'
      'FROM VEDOMOST Vedomost'
      '   INNER JOIN EDU_PLAN Edu_plan'
      '   ON  (Vedomost.N_PLAN = Edu_plan.N_PLAN)  '
      '   INNER JOIN OBJECT_PLAN Object_plan'
      '   ON  (Edu_plan.N_OBJECT = Object_plan.N_OBJECT) '
      '   INNER JOIN INCREMENT_GROUP Increment_group'
      '   ON (Increment_group.INC_GROUP = Vedomost.INC_GROUP)'
      '   INNER JOIN EDU_CARD Edu_card'
      '   ON (Edu_card.N_VED = Vedomost.N_VED) '
      '   INNER JOIN PEOPLE People'
      '   ON (People.NOMER = Edu_card.NOMER) '
      
        'WHERE (Vedomost.N_FAC=:N_FAC) AND (Vedomost.INVALID = 1) AND (Ed' +
        'u_plan.VID_EDU_PLAN=:VID_EDU) AND (Vedomost.STATUS=:STATUS) AND ' +
        '(Vedomost.D_VED BETWEEN :D1 AND :D2)')
    Left = 432
    Top = 912
    ParamData = <
      item
        Name = 'N_FAC'
        DataType = ftSmallint
        ParamType = ptInput
        Value = Null
      end
      item
        Name = 'VID_EDU'
        DataType = ftSmallint
        ParamType = ptInput
      end
      item
        Name = 'STATUS'
        DataType = ftSmallint
        ParamType = ptInput
      end
      item
        Name = 'D1'
        DataType = ftSmallint
        ParamType = ptInput
      end
      item
        Name = 'D2'
        DataType = ftSmallint
        ParamType = ptInput
      end>
    object VedListJournalN_VED: TIntegerField
      FieldName = 'N_VED'
      Origin = 'N_VED'
      ProviderFlags = [pfInUpdate, pfInWhere, pfInKey]
    end
    object VedListJournalN_VEDOMOST: TSmallintField
      FieldName = 'N_VEDOMOST'
      Origin = 'N_VEDOMOST'
    end
    object VedListJournalNAME_OBJECT: TStringField
      AutoGenerateValue = arDefault
      FieldName = 'NAME_OBJECT'
      Origin = 'NAME_OBJECT'
      ProviderFlags = []
      ReadOnly = True
      Size = 250
    end
    object VedListJournalN_GROUP: TStringField
      AutoGenerateValue = arDefault
      FieldName = 'N_GROUP'
      Origin = 'N_GROUP'
      ProviderFlags = []
      ReadOnly = True
      Size = 10
    end
    object VedListJournalFIO_EXAM: TStringField
      FieldName = 'FIO_EXAM'
      Origin = 'FIO_EXAM'
      Size = 40
    end
    object VedListJournalFIO: TStringField
      AutoGenerateValue = arDefault
      FieldName = 'FIO'
      Origin = 'FIO'
      ProviderFlags = []
      ReadOnly = True
      Size = 94
    end
    object VedListJournalD_VED: TSQLTimeStampField
      FieldName = 'D_VED'
      Origin = 'D_VED'
    end
