  object RestoreFDUpdateSQL: TFDUpdateSQL
    Connection = FDConnection1
    InsertSQL.Strings = (
      'insert into RESTORE'
      '  (NOMER, N_CAUSE_RESTORE, VID_RESTORE, N_PRIKAZ_RES, '
      'D_PRIKAZ_RES, DESCRIBE)'
      'values'
      '  (:NOMER, :N_CAUSE_RESTORE, :VID_RESTORE, :N_PRIKAZ_RES, '
      ':D_PRIKAZ_RES, '
      '   :DESCRIBE)'
      '')
    ModifySQL.Strings = (
      'update RESTORE'
      'set'
      '  NOMER = :NOMER,'
      '  N_CAUSE_RESTORE = :N_CAUSE_RESTORE,'
      '  VID_RESTORE = :VID_RESTORE,'
      '  N_PRIKAZ_RES = :N_PRIKAZ_RES,'
      '  D_PRIKAZ_RES = :D_PRIKAZ_RES,'
      '  DESCRIBE = :DESCRIBE'
      'where'
      '  NOMER = :OLD_NOMER and'
      '  N_CAUSE_RESTORE = :OLD_N_CAUSE_RESTORE and'
      '  VID_RESTORE = :OLD_VID_RESTORE and'
      '  N_PRIKAZ_RES = :OLD_N_PRIKAZ_RES and'
      '  D_PRIKAZ_RES = :OLD_D_PRIKAZ_RES'
      '')
    DeleteSQL.Strings = (
      'delete from RESTORE'
      'where'
      '  NOMER = :OLD_NOMER and'
      '  N_CAUSE_RESTORE = :OLD_N_CAUSE_RESTORE and'
      '  VID_RESTORE = :OLD_VID_RESTORE and'
      '  N_PRIKAZ_RES = :OLD_N_PRIKAZ_RES and'
      '  D_PRIKAZ_RES = :OLD_D_PRIKAZ_RES'
      '')
