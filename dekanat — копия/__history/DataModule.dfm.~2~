  object Vedomost_ReportFDQuery: TFDQuery
    Connection = FDConnection1
    SQL.Strings = (
      
        'SELECT Vedomost.N_VEDOMOST, Vedomost.D_VED, Edu_plan.CLOCK_PLAN_' +
        'ALL, Edu_plan.CLOCK_PLAN,'
      
        'Vedomost.SEMESTR_VED, Vedomost.FIO_EXAM, Object_plan.NAME_OBJECT' +
        ','
      
        'People.FAM, People.NAME, People.OTCH, People.N_ZACHET, Vedomost.' +
        'N_VED,'
      
        'Vedomost.N_FAC, Vedomost.END_D_EXAM, Vedomost.STATUS, Increment_' +
        'group.N_GROUP,'
      
        'Increment_group.VID_EDU, Vedomost.N_SPECIALIZ, Edu_plan.ZACH_EXA' +
        'M,'
      'Edu_plan.YEAR_PLAN'
      'FROM VEDOMOST Vedomost'
      '   INNER JOIN EDU_PLAN Edu_plan'
      '   ON  (Vedomost.N_PLAN = Edu_plan.N_PLAN)  '
      '   INNER JOIN EDU_CARD Edu_card'
      '   ON  (Edu_card.N_VED = Vedomost.N_VED)  '
      '   INNER JOIN OBJECT_PLAN Object_plan'
      '   ON  (Edu_plan.N_OBJECT = Object_plan.N_OBJECT)  '
      '   INNER JOIN PEOPLE People'
      '   ON  (Edu_card.NOMER = People.NOMER)  '
      '   INNER JOIN INCREMENT_GROUP Increment_group'
      '   ON  (Vedomost.INC_GROUP = Increment_group.INC_GROUP)  '
      
        'WHERE (Vedomost.N_VED=:N_VEDOM) AND (Vedomost.N_FAC=:N_FAC) AND ' +
        '(Vedomost.INVALID = 0)'
      'ORDER BY People.FAM, People.NAME, People.OTCH')
    Left = 856
    Top = 104
    ParamData = <
      item
        Name = 'N_VEDOM'
        DataType = ftInteger
        ParamType = ptInput
      end
