    Connection = FDConnection1
    InsertSQL.Strings = (
      'insert into HOLIDAY'
      
        '  (NOMER, N_PRIKAZ_HOL, D_PRIKAZ_HOL, D_BEGIN_HOL, D_END_HOL, N_' +
        'HOL, TYPE_HOL)'
      'values'
      
