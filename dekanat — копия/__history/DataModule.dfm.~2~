      '  ADDRESS_BEFORE = :ADDRESS_BEFORE,'
      '  ADDRESS = :ADDRESS,'
      '  ADDRESS_LIFE = :ADDRESS_LIFE,'
      '  PHONE = :PHONE,'
      '  HOSTEL = :HOSTEL,'
      '  NAME_EDUC = :NAME_EDUC,'
      '  WHEN_EDUC = :WHEN_EDUC,'
      '  WHERE_EDUC = :WHERE_EDUC,'
