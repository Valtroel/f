  object Ball_FDQuery: TFDQuery
    OnCalcFields = Ball_FDQueryCalcFields
    CachedUpdates = True
    Connection = FDConnection1
    UpdateObject = Ball_FDUpdateSQL
    SQL.Strings = (
      
        'SELECT Edu_card.BALL, People.NOMER, People.FAM, People.NAME, Peo' +
        'ple.OTCH, Vedomost.N_VED, Vedomost.N_VEDOMOST, Edu_plan.ZACH_EXA' +
        'M, Object_plan.NAME_OBJECT, Edu_plan.N_PLAN, Edu_card.D_EXAM, Ve' +
        'domost.N_FAC'
      'FROM EDU_CARD Edu_card'
      '   INNER JOIN PEOPLE People'
      '   ON  (Edu_card.NOMER = People.NOMER)  '
      '   INNER JOIN VEDOMOST Vedomost'
      '   ON  (Edu_card.N_VED = Vedomost.N_VED)  '
      '   INNER JOIN EDU_PLAN Edu_plan'
      '   ON  (Vedomost.N_PLAN = Edu_plan.N_PLAN)  '
      '   INNER JOIN OBJECT_PLAN Object_plan'
      '   ON  (Edu_plan.N_OBJECT = Object_plan.N_OBJECT)  '
      
        'WHERE (Vedomost.N_VED=:N_VEDOM) AND (Vedomost.N_FAC=:N_FAC) AND ' +
        '(Vedomost.INVALID = 0)'
      'ORDER BY People.FAM, People.NAME, People.OTCH'
      '')
    Left = 344
    Top = 248
    ParamData = <
      item
        Name = 'N_VEDOM'
        DataType = ftInteger
        ParamType = ptInput
      end
      item
        Name = 'N_FAC'
        DataType = ftSmallint
        ParamType = ptInput
      end>
