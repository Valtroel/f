  object PassFDUpdateSQL: TFDUpdateSQL
    Connection = FDConnection1
    InsertSQL.Strings = (
      'insert into PASS'
      '  (NOMER, N_CAUSE, D_PASS, HOUR_PASS)'
      'values'
      '  (:NOMER, :N_CAUSE, :D_PASS, :HOUR_PASS)'
      '')
    ModifySQL.Strings = (
      'update PASS'
      'set'
      '  NOMER = :NOMER,'
      '  N_CAUSE = :N_CAUSE,'
      '  D_PASS = :D_PASS,'
      '  HOUR_PASS = :HOUR_PASS'
      'where'
      '  NOMER = :OLD_NOMER and'
      '  N_CAUSE = :OLD_N_CAUSE and'
      '  D_PASS = :OLD_D_PASS'
      '')
    DeleteSQL.Strings = (
      'delete from PASS'
      'where'
      '  NOMER = :OLD_NOMER and'
      '  N_CAUSE = :OLD_N_CAUSE and'
      '  D_PASS = :OLD_D_PASS'
      ' '
      '')
    Left = 856
    Top = 200
  end
  object Statistic_Pass_DateFDQuery: TFDQuery
    Connection = FDConnection1
    SQL.Strings = (
      'SELECT  SUM( HOUR_PASS )'
      'FROM PASS Pass'
      'WHERE (Pass.NOMER=:NOMER)'
      'AND'
      '(Pass.D_PASS BETWEEN :START AND :END)'
      'AND'
      '(Pass.N_CAUSE=:CAUSE)'
      ''
      '')
    Left = 976
    Top = 152
    ParamData = <
      item
        Name = 'NOMER'
        DataType = ftInteger
        ParamType = ptInput
      end
      item
        Name = 'START'
        DataType = ftTimeStamp
        ParamType = ptInput
      end
      item
        Name = 'END'
        DataType = ftTimeStamp
        ParamType = ptInput
      end
      item
        Name = 'CAUSE'
        DataType = ftSmallint
        ParamType = ptInput
      end>
    object Statistic_Pass_DateFDQuerySUM: TLargeintField
      AutoGenerateValue = arDefault
      FieldName = 'SUM'
      Origin = '"SUM"'
      ProviderFlags = []
      ReadOnly = True
    end
  end
  object People_KursFDQuery: TFDQuery
    CachedUpdates = True
    Connection = FDConnection1
    UpdateObject = People_KursFDSQL
    SQL.Strings = (
      
        'SELECT NOMER, FAM, NAME, OTCH, KURS, INC_GROUP, STATUS_KURS, N_G' +
        'ROUP'
      'FROM PEOPLE People'
      'WHERE (INC_GROUP=:GROUP)'
      'ORDER BY N_GROUP, FAM, NAME, OTCH'
