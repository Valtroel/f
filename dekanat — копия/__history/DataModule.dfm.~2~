  object Ved_exlistFDQuery: TFDQuery
    OnCalcFields = Ved_exlistFDQueryCalcFields
    Connection = FDConnection1
    SQL.Strings = (
      
        'SELECT DISTINCT Vedomost.N_VED, Vedomost.N_VEDOMOST, Vedomost.D_' +
        'VED, Vedomost.SEMESTR_VED, Vedomost.FIO_EXAM, Vedomost.N_PLAN, V' +
        'edomost.N_FAC, Vedomost.END_D_EXAM, People.FAM, People.NAME, Peo' +
        'ple.OTCH, Object_plan.NAME_OBJECT, Edu_plan.SEMESTR, Edu_plan.YE' +
        'AR_PLAN, People.VID_EDU, People.N_GROUP'
      'FROM VEDOMOST Vedomost'
      '   INNER JOIN EDU_CARD Edu_card'
      '   ON  (Vedomost.N_VED = Edu_card.N_VED)  '
      '   INNER JOIN EDU_PLAN Edu_plan'
      '   ON  (Vedomost.N_PLAN = Edu_plan.N_PLAN)  '
      '   INNER JOIN PEOPLE People'
      '   ON  (Edu_card.NOMER = People.NOMER)  '
      '   INNER JOIN OBJECT_PLAN Object_plan'
      '   ON  (Edu_plan.N_OBJECT = Object_plan.N_OBJECT)  '
      'WHERE  Vedomost.STATUS IN (1,3) AND (Vedomost.INVALID = 0)'
      'ORDER BY Vedomost.N_VEDOMOST'
      '')
    Left = 736
    Top = 152
