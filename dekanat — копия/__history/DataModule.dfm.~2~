  object PeopleFDUpdateSQL: TFDUpdateSQL
    Connection = FDConnection1
    InsertSQL.Strings = (
      'insert into PEOPLE'
      '  (NOMER, FAM, NAME, OTCH, POL, NAZ, PAPA, MAMA, MARIAGE, '
      
        'N_CHILD, EDUCATION, ARMY, WORK_BEFORE, ADDRESS_BEFORE, ADDRESS, ' +
        'ADDRESS_LIFE, PHONE, '
      
        'HOSTEL, NAME_EDUC, WHEN_EDUC, WHERE_EDUC, MIDDLE_B, N_VID_SPORT,' +
        ' N_DELA, VID_EDU,'
      
        'N_FAC, KURS, N_GROUP, N_SPEC, N_SPECIALIZ, N_PRIKAZ_KURS, EDU_WO' +
        'RK, D_PRIKAZ_KURS,'
      
        'N_FOUND, N_PRIKAZ_INCLUDE, D_PRIKAZ_INCLUDE, N_ZACHET, STATUS_PE' +
        'OPLE, INC_GROUP,'
      
        'DATA_R, VID_EDU_INCLUDE, N_FOUND_INCLUDE, SCHOLARSHIP, CITIZENSH' +
        'IP, N_DOGOVOR, '
      
        'PASPORT, PERSONAL_NOMER, D_INCLUDE, DATE_END, NUMBER, HOUSE, FLA' +
        'T, POSTCODE, ID_STREET)'
      'values (:NOMER, :FAM, :NAME, :OTCH, :POL, :NAZ, :PAPA, :MAMA, '
      
        ':MARIAGE, :N_CHILD, :EDUCATION, :ARMY, :WORK_BEFORE, :ADDRESS_BE' +
        'FORE, '
      ':ADDRESS, :ADDRESS_LIFE, :PHONE, :HOSTEL, :NAME_EDUC, '
      
        ':WHEN_EDUC, :WHERE_EDUC, :MIDDLE_B, :N_VID_SPORT, :N_DELA, :VID_' +
        'EDU,'
      ':N_FAC, :KURS, :N_GROUP, :N_SPEC, :N_SPECIALIZ, :N_PRIKAZ_KURS,'
      ':EDU_WORK, :D_PRIKAZ_KURS, :N_FOUND, :N_PRIKAZ_INCLUDE,'
      
        ':D_PRIKAZ_INCLUDE, :N_ZACHET, :STATUS_PEOPLE, :INC_GROUP, :DATA_' +
        'R, '
      ':VID_EDU_INCLUDE, :N_FOUND_INCLUDE, :SCHOLARSHIP, :CITIZENSHIP, '
      ':N_DOGOVOR, :PASPORT, :PERSONAL_NOMER, :D_INCLUDE, :DATE_END,'
      ':NUMBER, :HOUSE, :FLAT, :POSTCODE, :ID_STREET)'
      '')
    ModifySQL.Strings = (
      'update PEOPLE'
      'set'
